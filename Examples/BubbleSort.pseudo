Scrie "Introduceti n: "
Citeste n (Intreg)

a <- Intreg[n]

Pentru i <- 0, n - 1 executa
  Citeste a[i] (Intreg)
Sf

Pentru i <- 0, n - 1 executa
  Pentru j <- i + 1, n - 1 executa
    Daca a[i] > a[j] atunci
      t <- a[i]
      a[i] <- a[j]
      a[j] <- t
    Sf Daca
  Sf Pentru
Sf Pentru

Scrie "Sirul sortat este..."

Pentru i <- 0, n - 1 executa
  Scrie a[i]
Sf